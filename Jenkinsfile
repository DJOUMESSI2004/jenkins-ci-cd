pipeline {
    agent any
    stages {
        
        stage('Build app') {
            steps {
                echo 'the app tis building.....!'
            }
        }

         stage('Test app') {
            steps {
                echo 'Testing the application......'
            }
        }


         stage('Deploy app') {
            steps {
                echo 'Deploy the application .....'
            }
        }
    }
}
